public class User {
    private int id;
    private String name;
    private String username;
    private String email;
    private Address address;

    // Getter dan Setter

    public static class Address {
        private String street;
        private String city;
        private String zipcode;
        private Geo geo;

        // Getter dan Setter

        public static class Geo {
            private String lat;
            private String lng;

            // Getter dan Setter
        }
    }
}
